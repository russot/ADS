if __name__=='__main__':
	r1__ = [0.010,0.2,0.22+0.22,0.36+0.36,0.36+0.62,1+0.36,1+0.82,2.2+0.18]
	r2__ = [0,3,15,30+30,210+30]
	k=[]
	k1=[]
	k2=[]
	r1=1
	for r in r1__:
		k1.append(((r+r1)/r1,r))

	for r in r2__:
		k2.append(((r+r1)/r1,r) )

	for i in k1:
		for j in k2:
			k.append((i[0]*j[0],i[1],j[1]))
			#k.append(kj)
	k.sort()
	index = 0
	j =k[1]
	for i in k:
		index +=1
		p1 = find(r1__, i[1])
		p2 = find(r2__,i[2])
		print '%03d:%03.3f %03.4f\t%s\t%s'%(
				index,
				i[0],
				i[0]/j[0],
				'R1=%03.3f@%d'%( r1__[p1],p1) ,
				'R2=%03.3f@%d'%( r2__[p2],p2) )
		j=i
		

